{
  "name": "vite-plugin-vue-dx",
  "type": "module",
  "version": "0.0.3",
  "description": "Improves dx by using various vite plugins out of the box",
  "author": "Fahadul Islam @dapotatoman",
  "license": "MIT",
  "keywords": [
    "vite-plugin",
    "vue",
    "dx"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "prepack": "pnpm unbuild",
    "release": "changelogen --release && git push --follow-tags && npm publish",
    "prerelease": "changelogen --prerelease --release && git push --follow-tags && npm publish --tag rc"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.10",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "fontaine": "^0.5.0",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-icons": "^0.19.3",
    "unplugin-turbo-console": "^1.10.4",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.2.7",
    "vite-plugin-vue-devtools": "^7.5.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "changelogen": "^0.5.7",
    "eslint": "^8.57.1",
    "type-fest": "^4.26.1",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.6.0"
  }
}